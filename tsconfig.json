{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "noImplicitAny": false,
    "moduleResolution": "node",
    "strictNullChecks": true,
    "skipLibCheck": true,
    "lib": [
      "es2017"
    ],
    "inlineSourceMap": true,
    "outDir": ".tscOutDir"
  },
  "exclude": [
    "node_modules"
  ]
}