{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "noImplicitAny": false,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "lib": [
      "dom",
      "es2017"
    ],
    "sourceMap": true
  },
  "exclude": [
    "node_modules",
    ".dest*",
    "spec/forIssues"
  ]
}