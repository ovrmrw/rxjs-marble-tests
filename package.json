{
  "name": "rxjs-marble-tests",
  "version": "1.0.0",
  "description": "RxJS Marble Tests.",
  "main": "index.js",
  "scripts": {
    "prebuild": "node build/clean-files.js",
    "build": "npm run tsc",
    "pretest": "npm run build",
    "test": "concurrently -k \"npm run mocha:w\" \"npm run tsc:w\" ",
    "tsc": "tsc --outDir .dest-test",
    "tsc:w": "tsc --outDir .dest-test --watch",
    "mocha": "mocha --require testing/main .dest-test/**/*.js",
    "mocha:w": "mocha --require testing/main .dest-test/**/*.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ovrmrw/rxjs-marble-tests.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ovrmrw/rxjs-marble-tests/issues"
  },
  "homepage": "https://github.com/ovrmrw/rxjs-marble-tests#readme",
  "dependencies": {
    "core-js": "^2.4.1",
    "lodash": "^4.16.4",
    "rxjs": "^5.0.0-rc.4",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/fs-extra": "0.0.33",
    "@types/lodash": "^4.14.37",
    "@types/mocha": "^2.2.33",
    "@types/node": "^6.0.45",
    "chai": "^3.5.0",
    "concurrently": "^3.1.0",
    "fs-extra": "^0.30.0",
    "mocha": "^3.2.0",
    "typescript": "^2.1.1"
  }
}
